import java.util.*;
import java.io.*;
class program{
       static void Converter(int args)
       {
        System.out.println(Math.abs(arg));
       }
       void doAction(){
        List<String> numbers = new ArrayList<String>();
        numbers.forEach(program::Converter);
       }
}

class extra{
    public static void main(String[] args) {
        program obj = new program();
        obj.doAction();
    }
}



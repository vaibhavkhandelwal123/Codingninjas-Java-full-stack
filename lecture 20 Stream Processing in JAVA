import java.util.*;
import java.lang.*;
import java.util.stream.Collector;
import java.util.stream.Collectors;
import java.io.*;
public class extra{
     public static void main(String[] args) {
        List<Integer> list = Array.asList(12,12,34,-43);
     List<Integer> result = list.stream().filter(e->e<0).collect(Collectors.toList());
     System.out.println(result);
     }
}

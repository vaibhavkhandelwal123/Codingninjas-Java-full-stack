import java.util.*;
public class extra{
    public static void main(String[] args) {
        HashMap map = new HashMap();
        map.put("australia", 2);
        map.put("india", 1);
        map.put("usa", 3);
        map.put("china", 4);
        map.put("japan", 5);

        System.out.println(map);
    }
}















import java.util.*;
public class extra{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
         HashSet set = new HashSet();
         set.add("red");
         set.add("orange");
         set.add("red");
         set.add("blue");
         set.add("green");

         System.out.println(set);

        }
    }





import java.util.*;
public class extra{
    public static void main(String[] args) {
        LinkedList ml=new LinkedList<>();
        ml.add(1);
        ml.add(2);
        ml.add(3);

        System.out.println(ml.get(1));
        Iterator it=ml.iterator();
        while (it.hasNext()) {
            it.remove();
            System.out.println(it.next());
        }
    }
}



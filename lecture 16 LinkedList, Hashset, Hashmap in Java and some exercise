import java.util.*;
public class extra{
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
         ArrayList<String> list = new ArrayList<String>();
         for(int i = 0; i <=5;i++)
         {
            String item=sc.next();
            if (list.contains(item)) {
                continue;
            }
            list.add(item);
         }
         System.out.println(list);

        }
    }



